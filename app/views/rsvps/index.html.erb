<h1>Listing rsvps</h1>

<table>
  <tr>
    <th>Viber</th>
    <th>Event</th>
    <th>Sincerity</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rsvps.each do |rsvp| %>
  <tr>
    <td><%= rsvp.viber_id %></td>
    <td><%= rsvp.event_id %></td>
    <td><%= rsvp.sincerity %></td>
    <td><%= rsvp.note %></td>
    <td><%= link_to 'Show', rsvp %></td>
    <td><%= link_to 'Edit', edit_rsvp_path(rsvp) %></td>
    <td><%= link_to 'Destroy', rsvp, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Rsvp', new_rsvp_path %>
